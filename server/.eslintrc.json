{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": ["projects/**/*"],
    "extends": ["../.eslintrc.json"],
    "rules": {
        "jsdoc/check-indentation": "off"
    },
    "globals": {
        "NodeJS": true,
        "Express": true,
        "Mocha": true
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    },
    "overrides": []
}
